{
  "name": "mederickbernier/agriops-lite",
  "type": "project",
  "require": {
    "predis/predis": "^2.2",
    "monolog/monolog": "^3.5",
    "nyholm/psr7": "^1.8",
    "ramsey/uuid": "^4.9"
  },
  "require-dev": {
    "pestphp/pest": "^3.0",
    "phpstan/phpstan": "^1.11",
    "rector/rector": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "test": "pest",
    "stan": "phpstan analyse --memory-limit=512M",
    "rector": "rector process --dry-run"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
